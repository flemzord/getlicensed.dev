{
  "name": "getlicensed.dev",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxt generate && drizzle-kit generate:sqlite",
    "preview": "npm run build && wrangler pages dev",
    "postinstall": "nuxt prepare",
    "deploy:production": "npm run build && wrangler d1 migrations apply getlicensed-prod --env=production --remote && wrangler pages deploy --env=production",
    "cf-typegen": "wrangler types",
    "fmt": "pnpm biome format . --write && pnpm biome check .",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:up": "drizzle-kit up:sqlite"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.20",
    "@heroicons/vue": "^2.1.3",
    "@nuxt/ui": "^2.15.2",
    "@nuxthub/core": "^0.5.11",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@tailwindcss/forms": "^0.5.7",
    "drizzle-orm": "^0.30.9",
    "nuxt": "^3.11.2",
    "nuxt-auth-utils": "^0.0.24",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@cloudflare/workers-types": "^4.20240419.0",
    "drizzle-kit": "^0.20.17",
    "nitro-cloudflare-dev": "^0.1.4",
    "typescript": "^5.4.5",
    "wrangler": "^3.51.2"
  }
}
